        <Button.Style>
            <Style TargetType="Button" BasedOn="{StaticResource MaterialDesignFloatingActionMiniButton}">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ElementName=saisonCalendarView, Path=SelectedIndex}" Value="0">
                        <Setter Property="IsEnabled" Value="False"/>
                        <Setter Property="ToolTip" Value=""/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ElementName=saisonCalendarView, Path=SelectedIndex}" Value="1">
                        <Setter Property="ToolTip" Value="Obst"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding ElementName=saisonCalendarView, Path=SelectedIndex}" Value="2">
                        <Setter Property="ToolTip" Value="Gemüse"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
        </Button.Style>
        <material:PackIcon Kind="ChevronLeft"/>
    </Button>
    <Button Click="SlideNext_Click" DockPanel.Dock="Right" Margin="8,0,4,0">
        <Button.Style>
            <Style TargetType="Button" BasedOn="{StaticResource MaterialDesignFloatingActionMiniButton}">
                <Setter Property="IsEnabled" Value="True"/>
                <Style.Triggers>
                    <DataTrigger Binding="{Binding ElementName=saisonCalendarView, Path=SelectedIndex}" Value="0">
