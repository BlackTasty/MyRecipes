                    </DataTrigger>
                    <DataTrigger Binding="{Binding ElementName=saisonCalendarView, Path=SelectedIndex}" Value="2">
                        <Setter Property="ToolTip" Value=""/>
                        <Setter Property="IsEnabled" Value="False"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
        </Button.Style>
        <material:PackIcon Kind="ChevronRight"/>
